};\n\n  const handleSubmit = async (event) => {\n    event.preventDefault();\n    try {\n      const { user } = await signInAuthUserWithEmailAndPassword(\n        email,\n        password\n      );\n      resetFormField();\n    } catch (error) {\n      switch (error.code) {\n        case \"auth/wrong-password\":\n          alert(\"Incorrect password for email.\");\n          break;\n        case \"auth/user-not-found\":\n          alert(\"no user associated with this email.\");\n          break;\n        default:\n          console.log(error);\n      }\n    }\n  };\n  const handleChange = (event) => {\n    const { name, value } = event.target;\n    setFormFields({ ...formFields, [name]: value });\n  };\n  return (\n    <div className=\"sign-up-container\">\n      <h2>Already have an account?</h2>\n      <span>Sign in with your email and password</span>\n      <form onSubmit={handleSubmit}>\n        <FormInput\n          label=\"Email\"\n          type=\"email\"\n          required\n          onChange={handleChange}\n          name=\"email\"\n          value={email}\n        />\n\n        <FormInput\n          label=\"Password\"\n          type=\"password\"\n          required\n          onChange={handleChange}\n          name=\"password\"\n          value={password}\n        />\n\n        <div className=\"buttons-container\">\n          <Button type=\"submit\">Sign In</Button>\n          <Button buttonType=\"google\" onClick={signInWithGoogle}>\n            Google Sign In\n          </Button>\n        </div>\n      </form>\n    </div>\n  );\n};\n\nexport default SignInForm;\n","/Users/Misaka/Vault/Grinnell/2021 Fall/Udemy SQL/React/react-app/clothings/clothing/src/contexts/user.context.jsx",["147"],[],"/Users/Misaka/Vault/Grinnell/2021 Fall/Udemy SQL/React/react-app/clothings/clothing/src/routes/shop/shop.component.jsx",[],[],"/Users/Misaka/Vault/Grinnell/2021 Fall/Udemy SQL/React/react-app/clothings/clothing/src/components/product-card/product-card.component.jsx",[